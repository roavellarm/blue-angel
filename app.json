{
  "expo": {
    "name": "Blue Angel",
    "slug": "blue-angel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#A4DCEB"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rodrigoavelar.blueangel"
    },
    "android": {
      "package": "com.rodrigoavelar.blueangel"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
